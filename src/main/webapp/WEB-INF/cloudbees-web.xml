<?xml version="1.0"?>
<cloudbees-web-app xmlns="http://www.cloudbees.com/xml/webapp/1">

	<!-- DataSources (use names refererenced via <resource-ref> in WEB-INF/web.xml) -->
	<resource name="jdbc/petclinic" auth="Container" type="javax.sql.DataSource">
		<param name="username" value="${cloudbees.datasource.username}" />
		<param name="password" value="${cloudbees.datasource.password}" />
		<param name="url" value="${cloudbees.datasource.url}" />
	</resource>

</cloudbees-web-app>
